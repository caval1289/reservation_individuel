{% extends 'base.html.twig' %}

{% block title %}Fiche d'un type
{% endblock %}

{% block body %}
	<h1>{{ type.type }}</h1>

	<h2>Liste des artistes</h2>
	<ul>
		{% for artistType in type.artists %}
			<li>{{ artistType.artist.firstname }}
				{{ artistType.artist.lastname }}</li>
		{% endfor %}
	</ul>
{% endblock %}
